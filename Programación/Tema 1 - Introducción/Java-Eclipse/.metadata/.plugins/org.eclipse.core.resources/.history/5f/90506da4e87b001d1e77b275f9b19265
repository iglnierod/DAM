package Objetos;

public class RectanguloTexto {
	private int base;
	private int altura;

	public void establecerBase(int valor) {
		base = valor;
	}
	
	public static void main(String[] args) {
		RectanguloTexto rt = new RectanguloTexto();
		
		rt.establecerBase(4);
		System.out.println(rt.base);
	}
}
